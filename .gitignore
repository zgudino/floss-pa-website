# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

.byebug_history
#ignore all public system and upload files
/public/system
/public/uploads
/public/system/*
/public/uploads/*

.env